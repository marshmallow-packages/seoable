{
    "name": "marshmallow/seoable",
    "description": "A Laravel Nova field which adds all SEO related meta fields to an Resource.",
    "license": "MIT",
    "type": "library",
    "homepage": "https://marshmallow.dev",
    "authors": [
        {
            "name": "Stef van Esch",
            "email": "stef@marshmallow.dev"
        }
    ],
    "keywords": [
        "laravel",
        "nova",
        "seoable"
    ],
    "require": {
        "php": "^7.1|^8.0",
        "marshmallow/commands": "^1.0",
        "marshmallow/mr-mallow": "^1.0",
        "marshmallow/nova-advanced-image": "^1.0",
        "marshmallow/nova-fields-help": "^1.0",
        "marshmallow/nova-flexible": "^1.0",
        "marshmallow/nova-charcounted-fields": "^1.0",
        "marshmallow/translatable": "^1.0",
        "marshmallow/sluggable": "^1.0",
        "marshmallow/nova-tags-field": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Seoable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Seoable\\ServiceProvider"
            ],
            "aliases": {
                "Seo": "Marshmallow\\Seoable\\Facades\\Seo"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": false
}
