{
    "name": "marshmallow/seoable",
    "description": "A Laravel Nova field which adds all SEO related meta fields to an Resource.",
    "license": "MIT",
    "type": "library",
    "homepage": "https://marshmallow.dev",
    "authors": [
        {
            "name": "Stef van Esch",
            "email": "stef@marshmallow.dev"
        }
    ],
    "keywords": [
        "laravel",
        "nova",
        "seoable"
    ],
    "require": {
        "php": "^8.0",
        "marshmallow/commands": "^1.0",
        "marshmallow/mr-mallow": "^1.0",
        "marshmallow/nova-advanced-image": "^2.0",
        "marshmallow/nova-flexible": "^2.0",
        "marshmallow/nova-charcounted-fields": "^2.0",
        "marshmallow/nova-tinymce": "^2.1",
        "marshmallow/nova-fields-help": "^v2.0",
        "marshmallow/translatable": "^2.0",
        "marshmallow/sluggable": "^1.0",
        "laravel/nova": "^4.0|^5.0",
        "marshmallow/nova-multiselect-field": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Seoable\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Seoable\\ServiceProvider"
            ],
            "aliases": {
                "Seo": "Marshmallow\\Seoable\\Facades\\Seo"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": false
}
