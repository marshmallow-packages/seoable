{
    "name": "marshmallow/seoable",
    "description": "A Laravel Nova field which adds all SEO related meta fields to an Resource.",
    "license": "MIT",
    "type": "library",
    "homepage": "https://marshmallow.dev",
    "authors": [
        {
            "name": "Stef van Esch",
            "email": "stef@marshmallow.dev"
        }
    ],
    "keywords": [
        "laravel",
        "nova",
        "seoable"
    ],
    "require": {
        "php": "^8.0",
        "marshmallow/commands": "^1.0",
        "marshmallow/mr-mallow": "^1.0",
        "marshmallow/nova-advanced-image": "v2.0",
        "marshmallow/nova-fields-help": "dev-nova4",
        "marshmallow/nova-flexible": "dev-nova4",
        "marshmallow/nova-charcounted-fields": "^v2.0",
        "marshmallow/translatable": "^v2.0",
        "marshmallow/sluggable": "^1.0",
        "laravel/nova": "^4.0",
        "marshmallow/nova-tags-field": "dev-nova4",
        "marshmallow/nova-multiselect-field": "dev-nova4"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Seoable\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "../*",
            "only": [
                "marshmallow/nova-flexible",
                "marshmallow/pages",
                "marshmallow/gtmetrix",
                "marshmallow/seoable",
                "marshmallow/nova-styling",
                "marshmallow/nova-fields-help",
                "marshmallow/nova-tinymce",
                "marshmallow/nova-tags-field",
                "marshmallow/nova-multiselect-field"
            ],
            "options": {
                "symlink": true
            }
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Seoable\\ServiceProvider"
            ],
            "aliases": {
                "Seo": "Marshmallow\\Seoable\\Facades\\Seo"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": false
}
